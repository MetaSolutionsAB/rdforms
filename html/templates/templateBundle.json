{
  "root": "test",
  "namespaces": {
    "ex": "http://example.com/",
    "cc": "http://creativecommons.org/licenses/"
  },
  "templates": [
    {
      "id": "test",
      "type": "group",
      "items": [
        {
          "label": {"en": "Documentation"},
          "nodetype": "RESOURCE",
          "id": "docheader",
          "type": "group",
          "styles": [
            "heading"
          ],
          "items": [
            {
              "type": "text",
              "nodetype": "LITERAL",
              "property": "dcterms:identifier",
              "cardinality": {
                "min": 1,
                "max": 1
              },
              "label": {
                "en": "Identifier",
                "sv": "Identifierare"
              },
              "styles": [
                "autoUUID",
                "nonEditable"
              ]
            },
            {
              "type": "text",
              "nodetype": "LANGUAGE_LITERAL",
              "property": "dcterms:title",
              "cardinality": {
                "min": 2,
                "pref": "4",
                "max": 5
              },
              "label": {
                "en": "Title",
                "sv": "Titel"
              },
              "description": {
                "en": "A short title of the resource",
                "sv": "En kort titel fÃ¶r resursen"
              },
              "styles": ["deprecated"]
            }
          ]
        },
        {
          "type": "text",
          "nodetype": "LANGUAGE_LITERAL",
          "property": "dcterms:description",
          "cardinality": {
            "pref": "1"
          },
          "label": {
            "en": "Description",
            "sv": "Beskrivning"
          },
          "styles": [
            "multiline"
          ]
        },
        {
          "type": "text",
          "nodetype": "DATATYPE_LITERAL",
          "property": "dcterms:modified",
          "datatype": "xsd:date",
          "cardinality": {
            "min": 1,
            "pref": "1",
            "max": 1
          },
          "label": {
            "en": "Modification date",
            "sv": "Modifieringsdatum"
          },
          "description": {
            "en": "A date when this resource was modified.",
            "sv": "Ett datum nÃ¤r denna resurs modifierades."
          },
          "styles": [
            "autoUpdateDate",
            "nonEditable"
          ]
        },
        {
          "type": "text",
          "nodetype": "DATATYPE_LITERAL",
          "property": "dcterms:date",
          "datatype": "xsd:date",
          "cardinality": {
            "min": 1,
            "pref": "1",
            "max": 1
          },
          "label": {
            "en": "Date",
            "sv": "Datum"
          },
          "description": {
            "en": "A date for this resource.",
            "sv": "Ett datum för denna resurs."
          },
          "styles": [
          ]
        },
        {
          "id": "subject",
          "type": "choice",
          "nodetype": "URI",
          "property": "http://purl.org/dc/terms/subject",
          "cardinality": {
            "min": 1,
            "pref": 1,
            "max": 1
          },
          "choices": [
            {
              "value": "ex:physics",
              "label": {
                "en": "Physics",
                "sv": "Fysik"
              }
            },
            {
              "value": "http://example.com/chemistry",
              "label": {
                "en": "Chemistry",
                "sv": "Kemi"
              },
              "description": {
                "en": "Chemistry is the field of study of atoms and their reactions."
              }
            }
          ],
          "label": {
            "en": "Subject",
            "sv": "Ämne"
          },
          "description": {
            "en": "A subject for this resource.",
            "sv": "Ett ämne för denna resurs."
          },
          "styles": ["verticalradiobuttons", "preserveOrderOfChoices"]
        },
        {
          "type": "choice",
          "nodetype": "URI",
          "property": "http://purl.org/dc/terms/license",
          "cardinality": {
            "min": 1,
            "pref": 1,
            "max": 3
          },
          "choices": [
            {
              "value": "cc:by/4.0/",
              "label": {
                "en": "CC BY 4.0 (Attribution)"
              }
            },
            {
              "value": "cc:by-nc/4.0/",
              "label": {
                "en": "CC BY-NC 4.0 (Attribution, Non-Commercial)"
              }
            }
          ],
          "styles": [
            "verticalRadioButtons"
          ],
          "label": {
            "en": "License",
            "sv": "Licens"
          },
          "description": {
            "en": "A license for this resource.",
            "sv": "En licens fÃ¶r denna resurs."
          }
        },
        {
          "type": "choice",
          "nodetype": "URI",
          "property": "http://purl.org/dc/terms/relation",
          "placeholder": "a relation",
          "cardinality": {
            "min": 1,
            "pref": 1,
            "max": 2
          },
          "label": {
            "en": "Relation",
            "sv": "Relation"
          },
          "description": {
            "en": "A relation to another resource.\nSome more information.",
            "sv": "En relation till en annan resurs."
          },
          "constraints": {
            "rdf:type": "skos:Concept"
          }
        },
        {
          "type": "group",
          "nodetype": "URI",
          "property": "http://www.w3.org/2000/01/rdf-schema#seeAlso",
          "constraints": {
            "rdf:type": "foaf:Document"
          },
          "label": {
            "en": "See also",
            "sv": "Se vidare"
          },
          "cardinality": {
            "min": 1
          },
          "styles": [
            "externalLink"
          ],
          "cls": [
            "cardgroup"
          ],
          "pattern": ".*\\..*\\/",
          "valueTemplate": "http://",
          "items": [
            {
              "type": "text",
              "nodetype": "URI",
              "label": {
                "en": "URI"
              },
              "description": {
                "en": "The web address you want to link to"
              }
            },
            {
              "type": "text",
              "nodetype": "LANGUAGE_LITERAL",
              "property": "dcterms:title",
              "cardinality": {
                "min": 1
              },
              "label": {
                "en": "Title",
                "sv": "Titel"
              },
              "description": {
                "en": "A short title of the resource",
                "sv": "En kort titel fÃ¶r resursen"
              }
            }
          ]
        },
        {
          "type": "text",
          "nodetype": "DATATYPE_LITERAL",
          "property": "dcterms:extent",
          "datatype": "xsd:duration",
          "cardinality": {
            "max": 1
          },
          "label": {
            "en": "Duration",
            "sv": "Tidsperiod"
          }
        },
        {
          "type": "group",
          "nodetype": "RESOURCE",
          "label": {
            "en": "Creator"
          },
          "property": "http://purl.org/dc/terms/creator",
          "cardinality": {
            "min": 0,
            "pref": 1,
            "max": 5
          },
          "constraints": {
            "rdf:type": "foaf:Person"
          },
          "cls": [
            "cardgroup"
          ],
          "deps": [
            "dcterms:subject",
            "http://example.com/physics"
          ],
          "items": [
            {
              "type": "text",
              "nodetype": "ONLY_LITERAL",
              "property": "http://xmlns.com/foaf/0.1/firstName",
              "cardinality": {
                "min": 1,
                "max": 1
              },
              "label": {
                "en": "First name"
              }
            },
            {
              "type": "text",
              "nodetype": "ONLY_LITERAL",
              "property": "http://xmlns.com/foaf/0.1/surname",
              "cardinality": {
                "min": 1
              },
              "label": {
                "en": "Surname"
              },
              "styles": ["deprecated"]
            }
          ]
        }
      ]
    }
  ]
}